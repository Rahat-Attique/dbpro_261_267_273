@model IEnumerable<LMS.Models.Student>

@{
    ViewBag.Title = "Allstt";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">View Student Details</h1>
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-heading">

                        <p>
                            <a class="btn btn-primary" href="@Url.Action("ReportStudent")">Download Report</a>
                        </p>

                        <table class="table">
                            <tr>
                                <th>
                                    @Html.DisplayNameFor(model => model.Name)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.FatherName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Address)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.PhoneNumber)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.EmailId)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.RegistrationNumber)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.CNIC)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.DOB)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Password)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Type)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Department.DepartmentName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Login.Email)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Sectionss.SectionName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Sectionss1.SectionName)
                                </th>
                                <th>
                                    @Html.DisplayNameFor(model => model.Session.Session1)
                                </th>
                                <th></th>
                            </tr>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Name)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.FatherName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Address)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PhoneNumber)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EmailId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.RegistrationNumber)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.CNIC)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.DOB)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Password)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Type)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Department.DepartmentName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Login.Email)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Sectionss.SectionName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Sectionss1.SectionName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Session.Session1)
                                    </td>

                                </tr>
                            }

                        </table>
